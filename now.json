{
  "version": 2,
  "alias": [
    "zoom-moda-api.now.sh"
  ],
  "env": {
    "TELEGRAM_TOKEN": "@telegram-token",
    "APP_TOKEN": "@app-token",
    "TELEGRAM_CHAT_ID": "-1001234543179",
    "TELEGRAM_CONNECT_TIMEOUT": "3s"
  },
  "project": "zoom-api-service",
  "builds": [
    {
      "src": "now/handler.go",
      "use": "@now/go",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/now/handler.go"
    }
  ]
}